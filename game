#include <iostream>
using namespace std;

void DrawMap(int HeroPos, char map[5]){
  
   for(int i=0; i<5; i++){
     
       if(i != HeroPos){
           
             cout<<map[i];
           
       }
       else{
           
             cout<<'H';
           
     }
   }
   
}

int main()
{
   
   int HeroPos = 0;
   char GameMap[5] = {'1','1','1','1','1'};
   char input = ' ';
   bool GameOver = false;
   
DrawMap(HeroPos, GameMap);


while(GameOver == false){
cin >> input;
   if(input == 'd'){
       
       HeroPos++;
   }
   else if(input == 'a'){
       
       HeroPos--;
   }
   else{
       GameOver = true;
       cout <<"Game Over";
   }
   DrawMap(HeroPos, GameMap);
}
 
    return 0;
}
